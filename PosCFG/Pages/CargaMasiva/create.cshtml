            <div class="form-group">
                <label asp-for="fullTerminalSystemAUX.Custom15" class="control-label">MIDES</label>
                <input asp-for="fullTerminalSystemAUX.Custom15" class="form-control" />
                <span asp-validation-for="fullTerminalSystemAUX.Custom15" class="text-danger"></span>
