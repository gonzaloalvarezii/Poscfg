@page
@model PosCFG.Pages.CargaMasiva.createModel
@{
    ViewData["Title"] = "Carga Masiva";
}

<h1>Cargar MASIVA de TERMINALES</h1>
<br/>
<br/>
<h4>Terminales</h4>
<hr />
<br/>
<form method="post">
    <div class="form-row">      
        <div class="col-md-8">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-row">
                    <div class="form-group col-md-4">
                        <label asp-for="cantTerminalesACargar" class="control-label">Cantidad de Terminales</label>
                        <input asp-for="cantTerminalesACargar" id="myInput2" class="form-control"/>
                        <span asp-validation-for="cantTerminalesACargar" class="text-danger"></span>
                    </div>
                </div>
            <div class="form-group">
                <label asp-for="fullTerminalSystemAUX.HeaderLine1" class="control-label">Nombre del Comercio</label>
                <input asp-for="fullTerminalSystemAUX.HeaderLine1" class="form-control" onkeyup="this.value = this.value.toUpperCase();" maxlength="24"/>
                <span asp-validation-for="fullTerminalSystemAUX.HeaderLine1" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="fullTerminalSystemAUX.HeaderLine2" class="control-label">Dirección</label>
                <input asp-for="fullTerminalSystemAUX.HeaderLine2" class="form-control" onkeyup="this.value = this.value.toUpperCase();" maxlength="24"/>
                <span asp-validation-for="fullTerminalSystemAUX.HeaderLine2" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="fullTerminalSystemAUX.HeaderLine3" class="control-label">RUT</label>
                <input asp-for="fullTerminalSystemAUX.HeaderLine3" class="form-control" maxlength="12"/>
                <span asp-validation-for="fullTerminalSystemAUX.HeaderLine3" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="fullTerminalSystemAUX.Custom4" class="control-label">VISANET</label>
                <input asp-for="fullTerminalSystemAUX.Custom4" class="form-control" />
                <span asp-validation-for="fullTerminalSystemAUX.Custom4" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="fullTerminalSystemAUX.Custom5" class="control-label">FIRSTDATA</label>
                <input asp-for="fullTerminalSystemAUX.Custom5" class="form-control" />
                <span asp-validation-for="fullTerminalSystemAUX.Custom5" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="fullTerminalSystemAUX.Custom6" class="control-label">OCA</label>
                <input asp-for="fullTerminalSystemAUX.Custom6" class="form-control" />
                <span asp-validation-for="fullTerminalSystemAUX.Custom6" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="fullTerminalSystemAUX.Custom7" class="control-label">CREDITOS DIRECTOS</label>
                <input asp-for="fullTerminalSystemAUX.Custom7" class="form-control" />
                <span asp-validation-for="fullTerminalSystemAUX.Custom7" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="fullTerminalSystemAUX.Custom8" class="control-label">CABAL U$ (PESOS)</label>
                <input asp-for="fullTerminalSystemAUX.Custom8" class="form-control" />
                <span asp-validation-for="fullTerminalSystemAUX.Custom8" class="text-danger"></span>
            </div>
            <div class="form-group">                
                <label asp-for="fullTerminalSystemAUX.Custom19" class="control-label">CABAL U$S (DOLARES)</label>
                <input asp-for="fullTerminalSystemAUX.Custom19" class="form-control" />
                <span asp-validation-for="fullTerminalSystemAUX.Custom19" class="text-danger"></span>
