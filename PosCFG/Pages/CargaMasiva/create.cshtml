                    <label asp-for="fullTerminalSystemAUX.ControlCheckSum" class="control-label">Actualizar Tipo Comercio (ControlChecksum)</label>
                    <input asp-for="fullTerminalSystemAUX.ControlCheckSum" class="form-check-input" checked />
                    <span asp-validation-for="fullTerminalSystemAUX.ControlCheckSum" class="text-danger"></span>
