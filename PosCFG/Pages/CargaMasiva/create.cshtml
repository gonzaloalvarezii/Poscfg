            <label class="control-label">JPOS</label>

            <div class="form-group">
                <label asp-for="fullTerminalSystemAUX.ca" class="control-label">CA (campo 43)</label>
                <input asp-for="fullTerminalSystemAUX.ca" class="form-control" maxlength="40"/>
                <span asp-validation-for="fullTerminalSystemAUX.ca" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="fullTerminalSystemAUX.mcc" class="control-label">MCC (campo 18)</label>
                <input asp-for="fullTerminalSystemAUX.mcc" class="form-control" maxlength="4" pattern="[A-Za-z0-9]+" title="Letras y números."/>
                <span asp-validation-for="fullTerminalSystemAUX.mcc" class="text-danger"></span>
