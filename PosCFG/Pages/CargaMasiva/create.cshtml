                    </div>
                    <div class="form-group">
                        <label asp-for="fullTerminalSystemAUX.ParameterVersion" class="control-label">Parameter Versión</label>
                        <input asp-for="fullTerminalSystemAUX.ParameterVersion" class="form-control" />
                        <span asp-validation-for="fullTerminalSystemAUX.ParameterVersion" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-4">
                    <label asp-for="fullTerminalSystemAUX.ParameterReload" class="control-label">Cargar parámetros de conexión (ParameterReload)</label>
                    <input asp-for="fullTerminalSystemAUX.ParameterReload" class="form-check-input" checked />
                    <span asp-validation-for="fullTerminalSystemAUX.ParameterReload" class="text-danger"></span>
                </div>
            </div>
            <div class="form-row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label asp-for="fullTerminalSystemAUX.ProgramID" class="control-label">Program ID</label>
                        <input asp-for="fullTerminalSystemAUX.ProgramID" class="form-control" />
                        <span asp-validation-for="fullTerminalSystemAUX.ProgramID" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="fullTerminalSystemAUX.ProgramVersion" class="control-label">Program Versión</label>
                        <input asp-for="fullTerminalSystemAUX.ProgramVersion" class="form-control" />
                        <span asp-validation-for="fullTerminalSystemAUX.ProgramVersion" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-6">
                    <label asp-for="fullTerminalSystemAUX.ProgramReload" class="control-label">Cargar programa</label>
                    <input asp-for="fullTerminalSystemAUX.ProgramReload" class="form-check-input" checked />
                    <span asp-validation-for="fullTerminalSystemAUX.ProgramReload" class="text-danger"></span>
                </div>
            </div>
            <div class="form-group">
                <label asp-for="fullTerminalSystemAUX.Paquete" class="control-label">Package Name</label>
                <input asp-for="fullTerminalSystemAUX.Paquete" class="form-control" />
                <span asp-validation-for="fullTerminalSystemAUX.Paquete" class="text-danger"></span>
            </div>
