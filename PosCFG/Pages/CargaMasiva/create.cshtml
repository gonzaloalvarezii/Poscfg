            <div class="form-group">
                <label asp-for="fullTerminalSystemAUX.Custom9" class="control-label">CREDITEL</label>
                <input asp-for="fullTerminalSystemAUX.Custom9" class="form-control" />
                <span asp-validation-for="fullTerminalSystemAUX.Custom9" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="fullTerminalSystemAUX.Custom10" class="control-label">PASSCARD</label>
                <input asp-for="fullTerminalSystemAUX.Custom10" class="form-control" />
                <span asp-validation-for="fullTerminalSystemAUX.Custom10" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="fullTerminalSystemAUX.Custom11" class="control-label">EDENRED</label>
                <input asp-for="fullTerminalSystemAUX.Custom11" class="form-control" />
                <span asp-validation-for="fullTerminalSystemAUX.Custom11" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="fullTerminalSystemAUX.Custom12" class="control-label">ANDA</label>
                <input asp-for="fullTerminalSystemAUX.Custom12" class="form-control" />
