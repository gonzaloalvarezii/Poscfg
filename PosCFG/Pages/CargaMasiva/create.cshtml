                    <label asp-for="fullTerminalSystemAUX.ParmConnChecksum" class="control-label">Actualizar parametros de conexión (ParmConnChecksum)</label>
                    <input asp-for="fullTerminalSystemAUX.ParmConnChecksum" class="form-check-input" checked />
                    <span asp-validation-for="fullTerminalSystemAUX.ParmConnChecksum" class="text-danger"></span>
                </div>
            </div>
            <div class="form-group">
                <label asp-for="fullTerminalSystemAUX.ControlGroup" class="control-label">Tipo de comercio</label>
                <input asp-for="fullTerminalSystemAUX.ControlGroup" class="form-control" />
