                <span asp-validation-for="fullTerminalSystemAUX.visa_spnsrd_mercht" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="fullTerminalSystemAUX.amex_id_comercio" class="control-label">AMEX Id Comercio</label>
                <input asp-for="fullTerminalSystemAUX.amex_id_comercio" class="form-control" maxlength="20" pattern="[A-Za-z0-9]+"  title="Letras y números."/>
